    "license": "MIT",
    "bugs": {
        "url": "https://github.com/geometryzen/stemcstudio-css-base/issues"
    },
    "homepage": "https://github.com/geometryzen/stemcstudio-css-base#readme",
    "devDependencies": {
        "uglifycss": "0.0.29"
    }
}